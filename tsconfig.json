{
  "compilerOptions": {
    "inlineSourceMap": false,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
